        function tem_verde()
            for i, v in pairs(game:GetService("Workspace").Casino.Computers:GetChildren()) do
                if v.Name == "Computer" then
                    if v.Display.Color == Color3.fromRGB(0, 255, 0) then
                        return true
                    end
                end
            end
            return false
        end
