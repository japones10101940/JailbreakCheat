	wait()
until Tp1 ~= nil and Tp2 ~= nil
function Tp(Vector)
	Tp1(Vector)
end

function getCamaro()
    local vehicle, spec, nearest = nil, nil, math.huge
    for i, v in next, require(game:GetService('ReplicatedStorage').Module.UI).CircleAction.Specs do
    
        if v.Name == "Enter Driver"  then
            if v.ValidRoot.Name == "Camaro" or v.ValidRoot.Name == 'Jeep' then
                local yourRoot = game.Players.LocalPlayer.Character.HumanoidRootPart
                local dist = (v.ValidRoot.PrimaryPart.Position - yourRoot.Position).Magnitude
        
                if dist < nearest then
                    vehicle, spec, nearest = v.ValidRoot, v, dist
                end
            end
        end
    end
    
    if vehicle ~= nil then
        return vehicle, spec, nearest
    else
        return nil
    end
end

function EntrarNoCarroMaisProximo()
    local carro, spec, nearest = getCamaro()

    spec:Callback(true)
end

