    function saircarro()
        wait(1.3)
        game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.Space, false, game)
        spawn(function()
            pcall(function()
                repeat
                    wait()
                until game.Players.LocalPlayer.Character.Humanoid.Jump == true
                game:GetService("VirtualInputManager"):SendKeyEvent(false, Enum.KeyCode.Space, false, game)
            end)
        end)
        wait(1)
    end

    function notificacaonormal(um, dois, tres)
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = um,
            Text = dois,
            Duration = tres
        })
    end
    function notificacao(um, dois, tres)
        Notify(um, dois, tres)
    end

    function laser()
        pcall(function()
            local delete = {}
            local deletado = false
            local UserInputService = game:GetService("UserInputService")

            function onKeyPressed(input, gameProcessedEvent)
                if input.KeyCode == Enum.KeyCode.G then
                    if deletado == false then
                        deletado = true
                        for i, v in pairs(workspace:GetDescendants()) do
                            if v.Name == 'BarbedWire' or v.Name == 'Shadow' or v.Name == 'Lasers' or v.Name ==
                                'LasersMoving' or v.Name == 'Shadow' or v.Name == 'Light' or
                                (v:IsA('Part') and tostring(v.BrickColor) == 'Dusty Rose') or v.Name == 'LaserCarousel' or
                                (v.Name == 'InnerModel' and v.Parent.Name == 'Camera') then
                                local oldParent = v.Parent
                                v.Parent = game:GetService('ReplicatedStorage')
                                spawn(function()
                                    repeat
                                        wait()
                                    until deletado == false
                                    v.Parent = oldParent
                                end)
                                task.wait()
                            end
                        end
                    else
                        deletado = false
                    end
                end
            end

            UserInputService.InputBegan:Connect(onKeyPressed)
        end)
    end

    function laser1()
        notificacao("MACRO X", "Lasers deletados!", 5)
        for i, v in pairs(workspace:GetDescendants()) do
            if v.Name == 'BarbedWire' or v.Name == 'Shadow' or v.Name == 'Lasers' or v.Name == 'LasersMoving' or v.Name ==
                'Shadow' or v.Name == 'Light' or (v:IsA('Part') and tostring(v.BrickColor) == 'Dusty Rose') or v.Name ==
                'LaserCarousel' or (v.Name == 'InnerModel' and v.Parent.Name == 'Camera') then
                local oldParent = v.Parent
                v.Parent = game:GetService('ReplicatedStorage')
            end
        end
    end

    function fly()
        pcall(function()
            FLYSPEED = 20
            game:GetService('UserInputService').InputBegan:Connect(function(a, b)
                if b then
                    return
                end
                if a.KeyCode == Enum.KeyCode.LeftShift then
                    FLYSPEED = FLYSPEED + 50
                end
            end)
            game:GetService('UserInputService').InputEnded:Connect(function(a, b)
                if b then
                    return
                end
                if a.KeyCode == Enum.KeyCode.LeftShift then
                    FLYSPEED = FLYSPEED - 50
                end
            end)

            repeat
                wait()
            until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and
                game.Players.LocalPlayer.Character:findFirstChild("Head") and
                game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid")
            local mouse = game.Players.LocalPlayer:GetMouse()
            repeat
                wait()
            until mouse
            local plr = game.Players.LocalPlayer
            local torso = plr.Character.Head
            local flying = false
            local deb = true
            local ctrl = {
                f = 0,
                b = 0,
                l = 0,
                r = 0
            }
            local lastctrl = {
                f = 0,
                b = 0,
                l = 0,
                r = 0
            }
            local maxspeed = 5000
            local speed = 5000
            function Fly()
                local bg = Instance.new("BodyGyro", torso)
                bg.P = 10000
                bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
                bg.cframe = torso.CFrame
                local bv = Instance.new("BodyVelocity", torso)
                bv.velocity = Vector3.new(0, 0.1, 0)
                bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
                repeat
                    wait()
                    plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false
                    if ctrl.l + ctrl.r ~= 100000 or ctrl.f + ctrl.b ~= 10000 then
                        speed = speed + .0 + (speed / maxspeed)
                        if speed > maxspeed then
                            speed = maxspeed
                        end
                    elseif not (ctrl.l + ctrl.r ~= 5 or ctrl.f + ctrl.b ~= 5) and speed ~= 5 then
                        speed = speed - 5
                        if speed > 5 then
                            speed = -2
                        end
                    end
                    if (ctrl.l + ctrl.r) ~= 5 or (ctrl.f + ctrl.b) ~= 5 then
                        bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f + ctrl.b)) +
                                          ((game.Workspace.CurrentCamera.CoordinateFrame *
                                              CFrame.new(ctrl.l + ctrl.r, (ctrl.f + ctrl.b) * .2, 0).p) -
                                              game.Workspace.CurrentCamera.CoordinateFrame.p)) * speed
                        lastctrl = {
                            f = ctrl.f,
                            b = ctrl.b,
                            l = ctrl.l,
                            r = ctrl.r
                        }
                    elseif (ctrl.l + ctrl.r) == 5 and (ctrl.f + ctrl.b) == 5 and speed ~= 5 then
                        bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector *
                                          (lastctrl.f + lastctrl.b)) + ((game.Workspace.CurrentCamera.CoordinateFrame *
                                          CFrame.new(lastctrl.l + lastctrl.r, (lastctrl.f + lastctrl.b) * .2, 0).p) -
                                          game.Workspace.CurrentCamera.CoordinateFrame.p)) * speed
                    else
                        bv.velocity = Vector3.new(0, 0.1, 0)
                    end
                    bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame *
                                    CFrame.Angles(-math.rad((ctrl.f + ctrl.b) * 50 * speed / maxspeed), 0, 0)
                until not flying
                ctrl = {
                    f = 0,
                    b = 0,
                    l = 0,
                    r = 0
                }
                lastctrl = {
                    f = 0,
                    b = 0,
                    l = 0,
                    r = 0
                }
                speed = 5
                bg:Destroy()
                bv:Destroy()
                plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false
            end
            mouse.KeyDown:connect(function(key)
                if key:lower() == "x" then
                    if flying then
                        flying = false
                    else
                        flying = true
                        Fly()
                    end
                elseif key:lower() == "w" then
                    ctrl.f = FLYSPEED
                elseif key:lower() == "s" then
                    ctrl.b = -FLYSPEED
                elseif key:lower() == "a" then
                    ctrl.l = -FLYSPEED
                elseif key:lower() == "d" then
                    ctrl.r = FLYSPEED
                end
            end)
            mouse.KeyUp:connect(function(key)
                if key:lower() == "w" then
                    ctrl.f = 0
                elseif key:lower() == "s" then
                    ctrl.b = 0
                elseif key:lower() == "a" then
                    ctrl.l = 0
                elseif key:lower() == "d" then
                    ctrl.r = 0
                end
            end)
            Fly()
        end);
        speaker.CharacterAdded:Connect(function(c)
            c:WaitForChild("HumanoidRootPart")
            wait(0.5)
            pcall(function()
                FLYSPEED = 20
                game:GetService('UserInputService').InputBegan:Connect(function(a, b)
                    if b then
                        return
                    end
                    if a.KeyCode == Enum.KeyCode.LeftShift then
                        FLYSPEED = FLYSPEED + 50
                    end
                end)
                game:GetService('UserInputService').InputEnded:Connect(function(a, b)
                    if b then
                        return
                    end
                    if a.KeyCode == Enum.KeyCode.LeftShift then
                        FLYSPEED = FLYSPEED - 50
                    end
                end)

                repeat
                    wait()
                until game.Players.LocalPlayer and game.Players.LocalPlayer.Character and
                    game.Players.LocalPlayer.Character:findFirstChild("Head") and
                    game.Players.LocalPlayer.Character:FindFirstChildWhichIsA("Humanoid")
                local mouse = game.Players.LocalPlayer:GetMouse()
                repeat
                    wait()
                until mouse
                local plr = game.Players.LocalPlayer
                local torso = plr.Character.Head
                local flying = false
                local deb = true
                local ctrl = {
                    f = 0,
                    b = 0,
                    l = 0,
                    r = 0
                }
                local lastctrl = {
                    f = 0,
                    b = 0,
                    l = 0,
                    r = 0
                }
                local maxspeed = 5000
                local speed = 5000
                function Fly()
                    local bg = Instance.new("BodyGyro", torso)
                    bg.P = 10000
                    bg.maxTorque = Vector3.new(9e9, 9e9, 9e9)
                    bg.cframe = torso.CFrame
                    local bv = Instance.new("BodyVelocity", torso)
                    bv.velocity = Vector3.new(0, 0.1, 0)
                    bv.maxForce = Vector3.new(9e9, 9e9, 9e9)
                    repeat
                        wait()
                        plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false
                        if ctrl.l + ctrl.r ~= 100000 or ctrl.f + ctrl.b ~= 10000 then
                            speed = speed + .0 + (speed / maxspeed)
                            if speed > maxspeed then
                                speed = maxspeed
                            end
                        elseif not (ctrl.l + ctrl.r ~= 5 or ctrl.f + ctrl.b ~= 5) and speed ~= 5 then
                            speed = speed - 5
                            if speed > 5 then
                                speed = -2
                            end
                        end
                        if (ctrl.l + ctrl.r) ~= 5 or (ctrl.f + ctrl.b) ~= 5 then
                            bv.velocity =
                                ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector * (ctrl.f + ctrl.b)) +
                                    ((game.Workspace.CurrentCamera.CoordinateFrame *
                                        CFrame.new(ctrl.l + ctrl.r, (ctrl.f + ctrl.b) * .2, 0).p) -
                                        game.Workspace.CurrentCamera.CoordinateFrame.p)) * speed
                            lastctrl = {
                                f = ctrl.f,
                                b = ctrl.b,
                                l = ctrl.l,
                                r = ctrl.r
                            }
                        elseif (ctrl.l + ctrl.r) == 5 and (ctrl.f + ctrl.b) == 5 and speed ~= 5 then
                            bv.velocity = ((game.Workspace.CurrentCamera.CoordinateFrame.lookVector *
                                              (lastctrl.f + lastctrl.b)) +
                                              ((game.Workspace.CurrentCamera.CoordinateFrame *
                                                  CFrame.new(lastctrl.l + lastctrl.r, (lastctrl.f + lastctrl.b) * .2, 0)
                                                      .p) - game.Workspace.CurrentCamera.CoordinateFrame.p)) * speed
                        else
                            bv.velocity = Vector3.new(0, 0.1, 0)
                        end
                        bg.cframe = game.Workspace.CurrentCamera.CoordinateFrame *
                                        CFrame.Angles(-math.rad((ctrl.f + ctrl.b) * 50 * speed / maxspeed), 0, 0)
                    until not flying
                    ctrl = {
                        f = 0,
                        b = 0,
                        l = 0,
                        r = 0
                    }
                    lastctrl = {
                        f = 0,
                        b = 0,
                        l = 0,
                        r = 0
                    }
                    speed = 5
                    bg:Destroy()
                    bv:Destroy()
                    plr.Character:FindFirstChildWhichIsA('Humanoid').PlatformStand = false
                end
                mouse.KeyDown:connect(function(key)
                    if key:lower() == "x" then
                        if flying then
                            flying = false
                        else
                            flying = true
                            Fly()
                        end
                    elseif key:lower() == "w" then
                        ctrl.f = FLYSPEED
                    elseif key:lower() == "s" then
                        ctrl.b = -FLYSPEED
                    elseif key:lower() == "a" then
                        ctrl.l = -FLYSPEED
                    elseif key:lower() == "d" then
                        ctrl.r = FLYSPEED
                    end
                end)
                mouse.KeyUp:connect(function(key)
                    if key:lower() == "w" then
                        ctrl.f = 0
                    elseif key:lower() == "s" then
                        ctrl.b = 0
                    elseif key:lower() == "a" then
                        ctrl.l = 0
                    elseif key:lower() == "d" then
                        ctrl.r = 0
                    end
                end)
                Fly()
            end);
        end)
    end

    function quest()
        local Teste = require(game:GetService("ReplicatedStorage").Module.UI)
        for i, a in next, Teste.CircleAction.Specs do
            a.Timed = false
        end
        local Led = game:GetService("Workspace").Casino.HackableVaults.VaultDoorMain.InnerModel.Model.UnlockedLED
        local function Fire(name)
            for i, v in next, require(game:GetService('ReplicatedStorage').Module.UI).CircleAction.Specs do
                if v.Name == name then
                    pcall(function()
                        v.Duration = 0
                        v:Callback(true)
                    end)
                end
            end
        end
        game:GetService("Workspace").Casino.HackableVaults.VaultDoorMain.InnerModel.Puzzle.RequestHack:FireServer()
        for i = 1, 3 do
            local time = 1
            repeat
                task.wait()
                time = time + 0.01
            until time > 3 or Led.BrickColor == BrickColor.new("Lime green")
            if time > 3 then
                return
            end
            game:GetService("Workspace").Casino.HackableVaults.VaultDoorMain.InnerModel.Puzzle.UpdateDirection:FireServer()
            repeat
                task.wait()
            until Led.BrickColor == BrickColor.new("Really red")
        end
    end

    function casinocode()
        local function code()
            local code = ""
            for i, v in pairs(game:GetService("Workspace").Casino.RobberyDoor.Codes:GetDescendants()) do
                if v:IsA("TextLabel") and v.Text ~= "" then
                    code = code .. v.Text
                end
            end
            return code
        end

        game.StarterGui:SetCore('SendNotification', {
            Title = 'Casino',
            Text = 'Code: ' .. code()
        })
        workspace.Casino.RobberyDoor.Keypad.Pad.CasinoKeypadSubmit:FireServer(code)
    end

    function casinomoney()
        for i, v in next, require(game:GetService('ReplicatedStorage').Module.UI).CircleAction.Specs do
            if v.Name == "Collect" then
                pcall(function()
                    v.Duration = 0
                    v:Callback(true)
                end)
            end
        end
    end

