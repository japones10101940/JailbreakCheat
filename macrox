    function soco()

        if not Punch then
            local NOME = 'attemptPunch'
            for i, v in next, getgc(true) do
                if type(v) == 'function' and getinfo(v).name == NOME then
                    getgenv().Punch = v
                end
            end
        end

        Punch()

        for i = 1, 5 do
            Punch()
            wait(0.65)
        end
    end

