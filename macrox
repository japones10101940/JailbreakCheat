    Farm:CriarBotao("Auto Airdrop", function()
        notificacao("MACRO X", "Risco de KICK!", 5)
        local Teste = require(game:GetService("ReplicatedStorage").Module.UI)
        for i, a in next, Teste.CircleAction.Specs do
            a.Timed = true
        end
        function drop(v)
            if v.Name == "Drop" then
                Tp2(v.Top.Position)
                saircarro()
                wait(1)

                spawn(function()
                    notificacao("MACRO X", "Colete o AirDrop!", 30)
                    notificacaonormal("MACRO X", "Colete o AirDrop!", 30)
                    no_clip(true)
                    while getHumanoid(speaker).Health > 0 do
                        getRoot(speaker).CFrame = v.Top.CFrame * CFrame.new(0, -3.9, 0)
                        no_clip(false)
                        local Teste = require(game:GetService("ReplicatedStorage").Module.UI)
                        for i, a in next, Teste.CircleAction.Specs do
                            a.Timed = true
                        end
                        task.wait()
                    end
                end)
            end
        end

        for i, v in pairs(workspace:GetChildren()) do
            if v.Name == "Drop" then
                drop(v)
            end
        end

    end)

