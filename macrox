    Combat:CriarLabel("Aimbot", true)
    local aimconfig = {
        aimbot = false,
        aimbot2 = false,
        teamcheck = false,
        fov = 0
    }
    game:GetService("UserInputService").InputBegan:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton2 then
            aimconfig.aimbot2 = true
        end
    end)
    game:GetService("UserInputService").InputEnded:Connect(function(input)
        if input.UserInputType == Enum.UserInputType.MouseButton2 then
            aimconfig.aimbot2 = false
        end
    end)

    Combat:CriarToggle('Aimbot', function(bool)
        aimconfig.aimbot = bool
    end)
    Combat:CriarToggle("Aimbot team check", function(bool)
        aimconfig.teamcheck = bool
    end)
    Combat:CriarSlider("Aim fov", 0, 120, function(bool)
        aimconfig.fov = bool
    end)

    game:GetService("RunService").RenderStepped:Connect(function()
        if aimconfig.aimbot == true and aimconfig.aimbot2 == true then
            workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position,
                getClosestPlayer(aimconfig.fov).Character.Head.Position)
        end
    end)

