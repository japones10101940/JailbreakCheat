        function botoes:CriarSlider(text, minvalue, maxvalue, callback)
            local TextButton = Instance.new("TextLabel")
            local UICorner = Instance.new("UICorner")
            local Valor = Instance.new("TextLabel")
            local BotaoPressionar = Instance.new("TextButton")
            local UICorner_2 = Instance.new("UICorner")
            local Vermelho = Instance.new("Frame")
            local UICorner_3 = Instance.new("UICorner")

            TextButton.Name = "TextButton"
            TextButton.Parent = ScrollingFrame
            TextButton.Active = true
            TextButton.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
            TextButton.Position = UDim2.new(0, 0, 0.375, 0)
            TextButton.Selectable = true
            TextButton.Size = UDim2.new(0, 320, 0, 37)
            TextButton.Font = Enum.Font.FredokaOne
            TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextButton.TextSize = 14.000
            TextButton.TextYAlignment = Enum.TextYAlignment.Top
            TextButton.Text = text

            UICorner.Parent = TextButton

            Valor.Name = "Valor"
            Valor.Parent = TextButton
            Valor.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Valor.BackgroundTransparency = 1.000
            Valor.Position = UDim2.new(0.903124988, 0, 0, 0)
            Valor.Size = UDim2.new(0, 30, 0, 37)
            Valor.Font = Enum.Font.FredokaOne
            Valor.Text = "0"
            Valor.TextColor3 = Color3.fromRGB(255, 255, 255)
            Valor.TextSize = 17.000
            Valor.TextWrapped = true 

            BotaoPressionar.Name = "BotaoPressionar"
            BotaoPressionar.Parent = TextButton
            BotaoPressionar.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            BotaoPressionar.Position = UDim2.new(0.0370000005, 0, 0.649999976, 0)
            BotaoPressionar.Size = UDim2.new(0, 260, 0, 3)
            BotaoPressionar.Font = Enum.Font.SourceSans
            BotaoPressionar.Text = ""
            BotaoPressionar.TextColor3 = Color3.fromRGB(0, 0, 0)
            BotaoPressionar.TextSize = 14.000

            UICorner_2.Parent = BotaoPressionar

            Vermelho.Name = "Vermelho"
            Vermelho.Parent = BotaoPressionar
            Vermelho.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            Vermelho.Size = UDim2.new(0, 0, 0, 3)

            UICorner_3.Parent = Vermelho

            local button = BotaoPressionar--your button

            local frame = Vermelho--your frame

            local TextLabel_2 = Valor--Your locationtovalue
            local uis = game:GetService("UserInputService")
            local minvalue = 0
            local maxvalue = 0
            local mouse = game.Players.LocalPlayer:GetMouse()

            button.MouseButton1Down:Connect(function()
                Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 260) * frame.AbsoluteSize.X) + tonumber(minvalue)) or 0
                pcall(function()
                    callback(Value)
                end)
                frame.Size = UDim2.new(0, math.clamp(mouse.X - frame.AbsolutePosition.X, 0, 260), 0, 3)
                moveconnection = mouse.Move:Connect(function()
                    TextLabel_2.Text = Value
                    Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 260) * frame.AbsoluteSize.X) + tonumber(minvalue))
                    pcall(function()
                        callback(Value)
                    end)
                    frame.Size = UDim2.new(0, math.clamp(mouse.X - frame.AbsolutePosition.X, 0, 260), 0, 3)
                end)
                releaseconnection = uis.InputEnded:Connect(function(Mouse)
                    if Mouse.UserInputType == Enum.UserInputType.MouseButton1 then
                        Value = math.floor((((tonumber(maxvalue) - tonumber(minvalue)) / 260) * frame.AbsoluteSize.X) + tonumber(minvalue))
                        pcall(function()
                            callback(Value)
                        end)
                        frame.Size = UDim2.new(0, math.clamp(mouse.X - frame.AbsolutePosition.X, 0, 260), 0, 3)
                        moveconnection:Disconnect()
                        releaseconnection:Disconnect()
                    end
                end)
            end)
        end
        
        return botoes 
    end
end

lib:Criar()
