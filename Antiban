        function botoes:CriarBotao(text,callback)
            local TextButton = Instance.new("TextButton")
            local UICorner = Instance.new("UICorner")
            local touch_app = Instance.new("ImageButton")
            local touch_app2 = Instance.new("ImageButton")
            local TextButton_Roundify_8px = Instance.new("ImageLabel")


            TextButton.Parent = ScrollingFrame
            TextButton.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
            TextButton.BackgroundTransparency = 1.000
            TextButton.BorderSizePixel = 0
            TextButton.Size = UDim2.new(0, 320, 0, 25)
            TextButton.ZIndex = 2
            TextButton.Font = Enum.Font.FredokaOne
            TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextButton.TextSize = 14.000
            TextButton.Text = text
            TextButton.MouseButton1Click:Connect(function()
                pcall(callback, TextButton)
            end)

            UICorner.Parent = TextButton

            touch_app.Name = "touch_app"
            touch_app.Parent = TextButton
            touch_app.BackgroundTransparency = 1.000
            touch_app.Position = UDim2.new(0.903125048, 0, -0.00216064602, 0)
            touch_app.Size = UDim2.new(0, 22, 0, 23)
            touch_app.ZIndex = 2
            touch_app.Image = "rbxassetid://3926305904"
            touch_app.ImageRectOffset = Vector2.new(84, 204)
            touch_app.ImageRectSize = Vector2.new(36, 36)

            touch_app2.Name = "touch_app2"
            touch_app2.Parent = touch_app
            touch_app2.AnchorPoint = Vector2.new(0.5, 0.5)
            touch_app2.BackgroundTransparency = 1.000
            touch_app2.Position = UDim2.new(0.5, 0, 0.5, 0)
            touch_app2.ZIndex = 2
            touch_app2.Image = "rbxassetid://3926305904"
            touch_app2.ImageColor3 = Color3.fromRGB(255, 0, 0)
            touch_app2.ImageRectOffset = Vector2.new(84, 204)
            touch_app2.ImageRectSize = Vector2.new(36, 36)

            TextButton_Roundify_8px.Name = "TextButton_Roundify_8px"
            TextButton_Roundify_8px.Parent = TextButton
            TextButton_Roundify_8px.Active = true
            TextButton_Roundify_8px.AnchorPoint = Vector2.new(0.5, 0.5)
            TextButton_Roundify_8px.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            TextButton_Roundify_8px.BackgroundTransparency = 1.000
            TextButton_Roundify_8px.Position = UDim2.new(0.5, 0, 0.5, 0)
            TextButton_Roundify_8px.Selectable = true
            TextButton_Roundify_8px.Size = UDim2.new(1, 0, 1, 0)
            TextButton_Roundify_8px.Image = "rbxassetid://3570695787"
            TextButton_Roundify_8px.ImageColor3 = Color3.fromRGB(24, 24, 24)
            TextButton_Roundify_8px.ScaleType = Enum.ScaleType.Slice
            TextButton_Roundify_8px.SliceCenter = Rect.new(100, 100, 100, 100)
            TextButton_Roundify_8px.SliceScale = 0.080


            local function AKTXW_fake_script() 
                local script = Instance.new('LocalScript', TextButton)

                script.Parent.MouseButton1Click:Connect(function()
                    spawn(function()
                        local clone = script.Parent.touch_app.touch_app2:Clone()
                
                        clone.Parent = script.Parent.touch_app
                
                        game:GetService("TweenService"):Create(clone, TweenInfo.new(0.3), {Size = UDim2.new(1, 0, 1, 0)}):Play()
                        wait(0.15)
                        game:GetService("TweenService"):Create(clone, TweenInfo.new(0.3), {ImageTransparency = 1}):Play()
                        wait(0.3)
                        clone:Destroy()
                    end)
                end)
            end
            coroutine.wrap(AKTXW_fake_script)()

        end
        function botoes:CriarLabel(text, underline)
            underline = underline or false 

            local TextButton = Instance.new("TextLabel")
            local UICorner = Instance.new("UICorner")
            local Frame = Instance.new("Frame")
            local Frame_2 = Instance.new("Frame")

            TextButton.Name = "TextButton"
            TextButton.Parent = ScrollingFrame
            TextButton.Active = true
            TextButton.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
            TextButton.Selectable = true
            TextButton.Size = UDim2.new(0, 320, 0, 25)
            TextButton.Font = Enum.Font.FredokaOne
            TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextButton.TextSize = 14.000
            TextButton.Text = text 

            UICorner.Parent = TextButton

            Frame.Parent = TextButton
            Frame.AnchorPoint = Vector2.new(0, 0.5)
            Frame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Frame.BorderSizePixel = 0
            Frame.Position = UDim2.new(0, 0, 0.5, 0)
            Frame.Size = UDim2.new(0, 100, 0, 2)
            Frame.Visible = ((underline and true) or (underline == false and false))
            Frame_2.Parent = TextButton
            Frame_2.AnchorPoint = Vector2.new(0, 0.5)
            Frame_2.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
            Frame_2.BorderSizePixel = 0
            Frame_2.Position = UDim2.new(0.6875, 0, 0.5, 0)
            Frame_2.Size = UDim2.new(0, 100, 0, 2)
            Frame_2.Visible = ((underline and true) or (underline == false and false))

            local a = {}
            function a:Refresh(text)
                TextButton.Text = text
            end
            return a 
        end
        function botoes:CriarToggle(text, callback)
            local TextButton = Instance.new("TextButton")
            local UICorner = Instance.new("UICorner")
            local Cor = Instance.new("Frame")
            local UICorner_2 = Instance.new("UICorner")
            local DropShadow = Instance.new("Frame")
            local UICorner_3 = Instance.new("UICorner")
            local DropShadow_2 = Instance.new("Frame")
            local UICorner_4 = Instance.new("UICorner")
            local DropShadow_3 = Instance.new("Frame")
            local UICorner_5 = Instance.new("UICorner")
            local DropShadow_4 = Instance.new("Frame")
            local UICorner_6 = Instance.new("UICorner")
            local TextButton_Roundify_8px = Instance.new("ImageLabel")

            --Properties:
            local toggled = false
            TextButton.Parent = ScrollingFrame
            TextButton.BackgroundColor3 = Color3.fromRGB(24, 24, 24)
            TextButton.BackgroundTransparency = 1.000
            TextButton.BorderSizePixel = 0
            TextButton.Size = UDim2.new(0, 320, 0, 25)
            TextButton.ZIndex = 2
            TextButton.Font = Enum.Font.FredokaOne
            TextButton.TextColor3 = Color3.fromRGB(255, 255, 255)
            TextButton.TextSize = 14.000
            TextButton.Text = text 
            TextButton.MouseButton1Click:Connect(function()
                toggled = not toggled 
                game:GetService("TweenService"):Create(Cor, TweenInfo.new(0.5), {BackgroundColor3 = ( (toggled and Color3.fromRGB (0, 255, 0) ) or ( toggled == false and Color3.fromRGB (255, 0, 0) ) )}):Play()
                pcall(callback, toggled)
            end)

            UICorner.Parent = TextButton

            Cor.Name = "Cor"
            Cor.Parent = TextButton
            Cor.BackgroundColor3 = Color3.fromRGB(255, 0, 0)
            Cor.BorderSizePixel = 0
            Cor.Position = UDim2.new(0.900000036, 0, 0.199999988, 0)
